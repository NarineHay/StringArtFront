{"ast":null,"code":"import \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport RangeSlider from 'vue-range-slider';\n// you probably need to import built-in style\nimport { toBlob } from 'html-to-image';\nimport validator from 'validator';\n// import htmlToImage from 'html-to-image';\n// Функция для преобразования изображения в формат base64\nfunction convertImageToBase64(imagePath) {\n  return new Promise((resolve, reject) => {\n    const reader = new FileReader();\n    reader.onerror = error => reject(error);\n    reader.onloaded = () => resolve(reader.result);\n    reader.readAsDataURL(imagePath);\n    console.log(convertImageToBase64, 11);\n  });\n}\nfunction aaa(imagePath) {\n  fetch(imagePath).then(res => res.blob()).then(blob => {\n    // Read the Blob as DataURL using the FileReader API\n    const reader = new FileReader();\n    reader.onloadend = () => {\n      console.log(reader.result);\n      // Logs data:image/jpeg;base64,wL2dvYWwgbW9yZ...\n\n      // Convert to Base64 string\n      const base64 = getBase64StringFromDataURL(reader.result);\n      console.log(base64);\n      // Logs wL2dvYWwgbW9yZ...\n    };\n    reader.readAsDataURL(blob);\n  });\n}\n// const getBase64StringFromDataURL = (dataURL) =>\n//     dataURL.replace('data:', '').replace(/^.+,/, '');\n\nexport default {\n  name: 'GenerateView',\n  data: () => ({\n    step: 0,\n    sliderValue: 50,\n    file: null,\n    image: null,\n    imageType: 1,\n    // 1 = square 2 = circle\n    ctx: null,\n    canvasWidth: 300,\n    canvasHeight: 300,\n    scale: 10,\n    turn: 0,\n    brightness: 50,\n    contrast: 50,\n    thread: 4.5,\n    email: '',\n    // Добавляем переменную для хранения email\n    test: '',\n    finishedImage: '',\n    errorMessage: '',\n    successMessage: ''\n  }),\n  methods: {\n    validateEmail(email) {\n      console.log(email);\n      if (validator.isEmail(email)) {\n        this.successMessage = 'Email is valid!';\n        this.errorMessage = '';\n      } else {\n        this.errorMessage = 'Электронная почта недействительна. Пожалуйста, введите действительный адрес электронной почты..';\n        this.successMessage = '';\n      }\n    },\n    goToStep(stepNumber) {\n      this.step = stepNumber;\n      if (stepNumber == 3) {\n        convertImageToBase64(this.dataUrl);\n        //   const node = document.getElementById('lastImageResult');\n        // node.style.border = 'none';\n\n        // toBlob(node)\n        //   .then((blob) => {\n        //     const reader = new FileReader();\n        //     reader.onloadend = () => {\n        //       const base64data = reader.result; // Get the base64 string without the data URI part\n        //       this.imageBase64 = base64data;\n        //       console.log('Base64 string:', base64data);\n        //     };\n        //     reader.readAsDataURL(blob);\n\n        //     node.style.border = '1px solid #25E477';\n        //   })\n        //   .catch((error) => {\n        //     node.style.border = '1px solid #25E477';\n        //     console.error('oops, something went wrong!', error);\n        //   });\n        console.log(this.imageBase64, 88888);\n        //    const fImage = document.getElementById('finishedImage');\n        // this.finishedImage = fImage.src\n        //  console.log(fImage.src, 444)\n      }\n    },\n    saveImage() {\n      const node = document.getElementById('lastImageResult');\n      // const fImage = document.getElementById('finishedImage');\n      // this.finishedImage = fImage.src\n      // console.log(fImage.src, 555)\n      node.style.border = 'none';\n      toBlob(node).then(function (dataUrl) {\n        const blobUrl = URL.createObjectURL(dataUrl);\n        const link = document.createElement(\"a\"); // Or maybe get it from the current document\n        link.setAttribute('id', 'lastImageResultDownload');\n        // ==================\n        console.log(aaa(dataUrl));\n        // ====================\n\n        link.href = blobUrl;\n        link.download = \"image.png\";\n        document.body.appendChild(link); // Or append it wherever you want\n        document.getElementById('lastImageResultDownload').click();\n        node.style.border = '1px solid #25E477';\n      }).catch(function (error) {\n        node.style.border = '1px solid #25E477';\n        console.error('oops, something went wrong!', error);\n      });\n    },\n    uploadFile() {\n      const filesSelected = this.$refs.file.files;\n      const t = this;\n      if (filesSelected.length > 0) {\n        let fileToLoad = filesSelected[0];\n        let fileReader = new FileReader();\n        fileReader.onload = function (fileLoadedEvent) {\n          let srcData = fileLoadedEvent.target.result; // <--- data: base64\n          t.file = srcData;\n        };\n        fileReader.readAsDataURL(fileToLoad);\n      }\n    },\n    uploadImageToCanvas() {\n      this.image = this.file;\n    },\n    // Функция для отправки данных на сервер\n    async sendDataToBackend() {\n      try {\n        this.email = document.getElementById('email').value;\n        this.validateEmail(this.email);\n\n        // console.log(this.image)\n        console.log(this.finishedImage, 888);\n        // Преобразуем изображения в формат base64\n        // const originalImageBase64 = await convertImageToBase64(this.file);\n        // const generatedImageBase64 = await convertImageToBase64(this.image);\n        //   console.log(originalImageBase64, 333);\n        //   console.log(generatedImageBase64, 111);\n\n        // Формируем данные для отправки\n        const data = {\n          email: this.email,\n          image: this.image,\n          generated_image: this.finishedImage\n        };\n\n        // Отправляем данные на сервер\n        const response = await fetch('http://backend.neteniti.ru/api/send-form', {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify(data)\n        });\n\n        // Получаем ответ от сервера\n        const responseData = await response.json();\n        console.log(responseData); // Выводим ответ в консоль\n        this.successMessage = 'Email is valid!';\n      } catch (error) {\n        this.errorMessage = 'An error occurred during validation.';\n        console.error('Ошибка:', error);\n      }\n    }\n\n    // Функция для преобразования изображения в формат base64\n    // imageToBase64(file) {\n    //     return new Promise((resolve, reject) => {\n    //         var reader = new FileReader();\n    //         reader.readAsDataURL(file);\n    //         reader.onload = () => {\n    //             resolve(reader.result);\n    //         };\n    //         reader.onerror = (error) => {\n    //             reject(error);\n    //         };\n    //     });\n    // }\n  },\n  watch: {},\n  mounted() {},\n  components: {\n    RangeSlider\n  }\n};","map":{"version":3,"names":["RangeSlider","toBlob","validator","convertImageToBase64","imagePath","Promise","resolve","reject","reader","FileReader","onerror","error","onloaded","result","readAsDataURL","console","log","aaa","fetch","then","res","blob","onloadend","base64","getBase64StringFromDataURL","name","data","step","sliderValue","file","image","imageType","ctx","canvasWidth","canvasHeight","scale","turn","brightness","contrast","thread","email","test","finishedImage","errorMessage","successMessage","methods","validateEmail","isEmail","goToStep","stepNumber","dataUrl","imageBase64","saveImage","node","document","getElementById","style","border","blobUrl","URL","createObjectURL","link","createElement","setAttribute","href","download","body","appendChild","click","catch","uploadFile","filesSelected","$refs","files","t","length","fileToLoad","fileReader","onload","fileLoadedEvent","srcData","target","uploadImageToCanvas","sendDataToBackend","value","generated_image","response","method","headers","JSON","stringify","responseData","json","watch","mounted","components"],"sources":["src/views/GenerateView.vue"],"sourcesContent":["<template>\n  <div class=\"generate\">\n    <div v-if=\"step === 0\" class=\"step step-0\">\n      <div class=\"step-title\">ШАГ 1</div>\n      <div class=\"step-label\">Выберите форму картины</div>\n      <div class=\"step-types\">\n        <div @click=\"imageType = 1\" class=\"step-types-square\">\n          <svg v-if=\"imageType === 1\" width=\"68\" height=\"50\" viewBox=\"0 0 68 50\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n          <path d=\"M67.0042 1.02678C65.6765 -0.301081 63.5238 -0.301081 62.1958 1.02678L21.4617 41.7613L5.80418 26.1038C4.47645 24.7759 2.32383 24.776 0.995833 26.1038C-0.332026 27.4315 -0.332026 29.5841 0.995833 30.912L19.0575 48.9734C20.3849 50.3011 22.5391 50.3002 23.8659 48.9734L67.0042 5.83512C68.332 4.50739 68.3319 2.35464 67.0042 1.02678Z\" fill=\"#25E477\"/>\n          </svg>\n        </div>\n        <div @click=\"imageType = 2\" class=\"step-types-circle\">\n          <svg v-if=\"imageType === 2\" width=\"68\" height=\"50\" viewBox=\"0 0 68 50\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n          <path d=\"M67.0042 1.02678C65.6765 -0.301081 63.5238 -0.301081 62.1958 1.02678L21.4617 41.7613L5.80418 26.1038C4.47645 24.7759 2.32383 24.776 0.995833 26.1038C-0.332026 27.4315 -0.332026 29.5841 0.995833 30.912L19.0575 48.9734C20.3849 50.3011 22.5391 50.3002 23.8659 48.9734L67.0042 5.83512C68.332 4.50739 68.3319 2.35464 67.0042 1.02678Z\" fill=\"#25E477\"/>\n          </svg>\n        </div>\n      </div>\n      <div class=\"step-file\">\n        <label for=\"file\" class=\"step-file-choose\">\n          <input ref=\"file\" @change=\"uploadFile\" type=\"file\" id=\"file\">\n           {{ file ? 'Файл выбран' : 'Выберите файл'  }}\n        </label>\n        <button @click=\"uploadImageToCanvas\" class=\"step-file-upload\">Загрузить</button>\n      </div>\n      <div class=\"step-image\" >\n        <div class=\"step-image-container\" :class=\"{'step-image-container-square': imageType === 1}\">\n          <img :src=\"image\" :style=\"{\n            transform: `scale(${scale / 10}) rotate(${turn}deg)`,\n            }\" alt=\"\">\n        </div>\n      </div>\n      <div class=\"step-scale\">\n        <span class=\"step-scale-title\">Масштаб</span>\n        <range-slider\n          class=\"slider\"\n          min=\"1\"\n          max=\"100\"\n          step=\"0.2\"\n          v-model=\"scale\">\n        </range-slider>\n      </div>\n      <div class=\"step-turn\">\n        <span class=\"step-turn-title\">Поворот</span>\n        <range-slider\n          class=\"slider\"\n          min=\"0\"\n          max=\"360\"\n          step=\"1\"\n          v-model=\"turn\">\n        </range-slider>\n      </div>\n      <div class=\"step-last\">\n        <button @click=\"step = 1\">К шагу 2</button>\n      </div>\n    </div>\n    <div v-if=\"step === 1\" class=\"step step-1\">\n      <div class=\"step-title\">ШАГ 2</div>\n      <div class=\"step-image\">\n        <div class=\"step-image-container\" :class=\"{'step-image-container-square': imageType === 1}\">\n          <img :src=\"image\" :style=\"{\n            transform: `scale(${scale / 10}) rotate(${turn}deg)`,\n            filter: `grayscale(1) brightness(${brightness}%) contrast(${contrast}%)`\n            }\" alt=\"\">\n        </div>\n      </div>\n      <div class=\"step-scale\">\n        <span class=\"step-scale-title\">Яркость: {{brightness}}%</span>\n        <range-slider\n          class=\"slider\"\n          min=\"10\"\n          max=\"180\"\n          step=\"1\"\n          v-model=\"brightness\">\n        </range-slider>\n      </div>\n      <div class=\"step-turn\">\n        <span class=\"step-turn-title\">Контраст: {{contrast}}%</span>\n        <range-slider\n          class=\"slider\"\n          min=\"10\"\n          max=\"180\"\n          step=\"1\"\n          v-model=\"contrast\">\n        </range-slider>\n      </div>\n      <div class=\"step-turn\">\n        <span class=\"step-turn-title\">Разброс нитей: {{thread}}</span>\n        <range-slider\n          class=\"slider\"\n          min=\"1\"\n          max=\"20\"\n          step=\"0.2\"\n          v-model=\"thread\">\n        </range-slider>\n      </div>\n      <div class=\"step-last\">\n        <button @click=\"step = 2\">К шагу 3</button>\n      </div>\n    </div>\n    <div v-if=\"step === 2\" class=\"step step-2\">\n      <div class=\"step-title\">ШАГ 3</div>\n      <div class=\"step-image\">\n        <div id=\"lastImageResult\" class=\"step-image-container\" :class=\"{'step-image-container-square': imageType === 1}\">\n          <img id=\"finishedImage\" :src=\"image\" :style=\"{\n            transform: `scale(${scale / 10}) rotate(${turn}deg)`,\n            filter: `grayscale(1) brightness(${brightness}%) contrast(${contrast}%)`\n            }\" alt=\"\">\n        </div>\n      </div>\n      <div class=\"step-generate\">\n        Сгенерировать изображение\n      </div>\n      <div class=\"step-btns\">\n        <div @click=\"saveImage\" class=\"step-btns-save\">\n          Сохранить изображение\n        </div>\n        <div @click=\"goToStep(3)\" class=\"step-btns-instructions\">\n          Инструкция\n        </div>\n      </div>\n    </div>\n    <div v-if=\"step === 3\" class=\"step step-2\">\n      <div class=\"step-title\">ИНСТРУКЦИЯ</div>\n      <div class=\"step-instructions-container\">\n        \n        Ваша инструкция по сборке готова!\n        Укажите Вашу почту.\n      </div>\n      <input type=\"email\" class=\"step-email\" placeholder=\"ТВОЙ E-MAIL\" id=\"email\">\n      <button class=\"step-send-email\" @click=\"sendDataToBackend\">Отправить почту</button>\n\n      <p v-if=\"errorMessage\" class=\"error text-danger\">{{ errorMessage }}</p>\n      <p v-if=\"successMessage\" class=\"success\">{{ successMessage }}</p>\n\n      <div class=\"step-help-container\">\n        При возникновении проблем просьба связаться через контактную форму.\n        просьба связаться через почту <router-link to=\"/\">почта@gmail.com</router-link>\n        <br>\n        Для упрощения изготовления картины вы можете воспользоваться нашим <router-link to=\"/\">помощником стринг арта</router-link>,  в который загружается файл с инструкцией который вам придет на почту, которую вы указали.\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport RangeSlider from 'vue-range-slider'\n// you probably need to import built-in style\nimport { toBlob } from 'html-to-image';\nimport validator from 'validator';\n// import htmlToImage from 'html-to-image';\n// Функция для преобразования изображения в формат base64\nfunction convertImageToBase64(imagePath) {\n    return new Promise((resolve, reject) => {\n        const reader = new FileReader();\n        \n        reader.onerror = error => reject(error);\n        reader.onloaded = () => resolve(reader.result);\n        reader.readAsDataURL(imagePath);\n        console.log(convertImageToBase64, 11)\n    });\n}\nfunction aaa(imagePath){\n  fetch(imagePath)\n        .then((res) => res.blob())\n        .then((blob) => {\n            // Read the Blob as DataURL using the FileReader API\n            const reader = new FileReader();\n            reader.onloadend = () => {\n                console.log(reader.result);\n                // Logs data:image/jpeg;base64,wL2dvYWwgbW9yZ...\n\n                // Convert to Base64 string\n                const base64 = getBase64StringFromDataURL(reader.result);\n                console.log(base64);\n                // Logs wL2dvYWwgbW9yZ...\n            };\n            reader.readAsDataURL(blob);\n        });\n}\n// const getBase64StringFromDataURL = (dataURL) =>\n//     dataURL.replace('data:', '').replace(/^.+,/, '');\n\n\nexport default {\n  name: 'GenerateView',\n  data: () => ({\n    step: 0,\n    sliderValue: 50,\n    file: null,\n    image: null,\n    imageType: 1, // 1 = square 2 = circle\n    ctx: null,\n    canvasWidth: 300,\n    canvasHeight: 300,\n    scale: 10,\n    turn: 0,\n    brightness: 50,\n    contrast: 50,\n    thread: 4.5,\n    email: '', // Добавляем переменную для хранения email\n    test: '',\n    finishedImage: '',\n    errorMessage: '',\n    successMessage: ''\n\n  }),\n  methods: {\n    validateEmail(email) {\n      console.log(email)\n      if (validator.isEmail(email)) {\n        this.successMessage = 'Email is valid!';\n        this.errorMessage = '';\n      } else {\n        this.errorMessage = 'Электронная почта недействительна. Пожалуйста, введите действительный адрес электронной почты..';\n        this.successMessage = '';\n      }\n    },\n    goToStep(stepNumber) {\n      this.step = stepNumber;\n      if(stepNumber == 3){\n        convertImageToBase64(this.dataUrl);\n      //   const node = document.getElementById('lastImageResult');\n      // node.style.border = 'none';\n\n      // toBlob(node)\n      //   .then((blob) => {\n      //     const reader = new FileReader();\n      //     reader.onloadend = () => {\n      //       const base64data = reader.result; // Get the base64 string without the data URI part\n      //       this.imageBase64 = base64data;\n      //       console.log('Base64 string:', base64data);\n      //     };\n      //     reader.readAsDataURL(blob);\n\n      //     node.style.border = '1px solid #25E477';\n      //   })\n      //   .catch((error) => {\n      //     node.style.border = '1px solid #25E477';\n      //     console.error('oops, something went wrong!', error);\n      //   });\n    console.log(this.imageBase64, 88888)\n      //    const fImage = document.getElementById('finishedImage');\n      // this.finishedImage = fImage.src\n      //  console.log(fImage.src, 444)\n      }\n    },\n    saveImage() {\n      const node = document.getElementById('lastImageResult');\n      // const fImage = document.getElementById('finishedImage');\n      // this.finishedImage = fImage.src\n      // console.log(fImage.src, 555)\n      node.style.border = 'none'\n      toBlob(node)\n        .then(function (dataUrl) {\n            const blobUrl = URL.createObjectURL(dataUrl);\n            const link = document.createElement(\"a\"); // Or maybe get it from the current document\n            link.setAttribute('id', 'lastImageResultDownload')\n            // ==================\n            console.log(aaa(dataUrl))\n            // ====================\n           \n            link.href = blobUrl;\n            link.download = \"image.png\";\n            document.body.appendChild(link); // Or append it wherever you want\n            document.getElementById('lastImageResultDownload').click() \n            node.style.border = '1px solid #25E477'\n        })\n        .catch(function (error) {\n          node.style.border = '1px solid #25E477'\n          console.error('oops, something went wrong!', error);\n        });\n    },\n    \n    uploadFile() {\n        const filesSelected = this.$refs.file.files\n        const t = this\n        if (filesSelected.length > 0) {\n          let fileToLoad = filesSelected[0];\n          let fileReader = new FileReader();\n          fileReader.onload = function(fileLoadedEvent) {\n            let srcData = fileLoadedEvent.target.result; // <--- data: base64\n            t.file = srcData\n          }\n          fileReader.readAsDataURL(fileToLoad);\n        }\n    },\n    \n    uploadImageToCanvas() {\n      this.image = this.file\n    },\n    \n    // Функция для отправки данных на сервер\n    async sendDataToBackend() {\n        try {\n     \n          this.email = document.getElementById('email').value;\n           \n          this.validateEmail(this.email)\n\n          // console.log(this.image)\n            console.log(this.finishedImage, 888)\n            // Преобразуем изображения в формат base64\n            // const originalImageBase64 = await convertImageToBase64(this.file);\n            // const ge